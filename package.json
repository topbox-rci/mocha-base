{
    "name": "@topbox-rci/mocha-base",
    "version": "3.0.1",
    "description": "Shared mocha stuff.",
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:topbox-rci/mocha-base.git"
    },
    "files": [
        "helpers"
    ],
    "scripts": {
        "test": "npm run lint && npm run unit",
        "lint": "eslint 'helpers/**/*.js' 'test/**/*.js'",
        "unit": "nyc --reporter=text-summary --reporter=lcov --reporter=cobertura mocha --exit \"test/**/*.spec.js\""
    },
    "nyc": {
        "temp-directory": "./coverage/.nyc_output"
    },
    "devDependencies": {
        "@topbox-rci/eslint-config": "https://github.com/topbox-rci/eslint-config.git#v1.0.0",
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "6.16.0",
        "babel-preset-stage-0": "6.16.0",
        "coveralls": "^3.0.4",
        "eslint": "3.19.0"
    },
    "dependencies": {
        "chai": "^4.2.0",
        "chance": "^1.0.18",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.12.1",
        "jsdom": "9.8.3",
        "mocha": "^5.2.0",
        "nyc": "^14.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "sinon": "3.2.0"
    }
}
